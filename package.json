{
  "name": "nest-shared",
  "version": "5.0.5",
  "description": "Shared code for nestjs projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "npx prettier --write \"src/**/*.(js|ts)\"",
    "lint": "npx eslint src --ext .js,.ts",
    "lint:fix": "npx eslint src --fix --ext .js,.ts",
    "test": "npx jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config jest-e2e.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "npx semantic-release",
    "release:dry": "npx semantic-release --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/nest-shared.git"
  },
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/nest-shared/issues"
  },
  "homepage": "https://github.com/hebertcisco/nest-shared#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@localtools/base64": "^0.0.4",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "aws-sdk": "^2.1655.0",
    "axios": "^1.7.2",
    "cache-manager": "^5.7.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-handle": "^0.0.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "qs": "^6.12.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "unique-slug": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "28.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "3.3.2",
    "semantic-release": "^24.0.0",
    "ts-jest": "29.1.5",
    "typescript": "5.5.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nestjs/cli": "^10.4.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.7",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.14.10",
    "@types/qs": "^6.9.15"
  }
}