{
  "name": "nest-shared",
  "version": "0.4.60",
  "description": "Shared code for nestjs projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config jest-e2e.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/nest-shared.git"
  },
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/nest-shared/issues"
  },
  "homepage": "https://github.com/hebertcisco/nest-shared#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@localtools/base64": "^0.0.4",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/platform-express": "^9.0.7",
    "@nestjs/swagger": "^6.0.4",
    "aws-sdk": "^2.1117.0",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-handle": "^0.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "nanoid": "^4.0.0",
    "nestjs-request-context": "^2.1.0",
    "oxide.ts": "^1.1.0",
    "qs": "^6.11.0",
    "randts": "^0.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "slonik": "^33.0.6",
    "unique-slug": "^2.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.6",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.12",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.6.2",
    "@types/qs": "^6.9.7",
    "@types/slonik": "^22.1.6",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.31.0",
    "eslint": "8.30.0",
    "eslint-plugin-jest": "27.1.6",
    "jest": "28.1.3",
    "prettier": "2.8.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  }
}
