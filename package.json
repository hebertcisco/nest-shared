{
  "name": "nest-shared",
  "version": "0.3.0",
  "description": "Shared code for nestjs projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/nest-shared.git"
  },
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/nest-shared/issues"
  },
  "homepage": "https://github.com/hebertcisco/nest-shared#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "axios": "^0.27.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "moment": "^2.29.2",
    "multer": "^1.4.4",
    "qs": "^6.10.3",
    "randts": "^0.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.4",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "26.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
