{
  "name": "nest-shared",
  "version": "5.0.9",
  "description": "Shared code for nestjs projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "npx prettier --write \"src/**/*.(js|ts)\"",
    "lint": "npx eslint src --ext .js,.ts",
    "lint:fix": "npx eslint src --fix --ext .js,.ts",
    "test": "npx jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config jest-e2e.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "npx semantic-release",
    "release:dry": "npx semantic-release --dry-run"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/nest-shared.git"
  },
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/nest-shared/issues"
  },
  "homepage": "https://github.com/hebertcisco/nest-shared#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@localtools/base64": "^0.0.4",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.1",
    "cache-manager": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-handle": "^0.0.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "unique-slug": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nestjs/cli": "^11.0.10",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/multer": "^2.0.0",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^24.3.3",
    "@types/qs": "^6.14.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "eslint": "^9.35.0",
    "eslint-plugin-jest": "29.0.1",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "semantic-release": "^24.2.8",
    "ts-jest": "29.4.1",
    "typescript": "5.9.2"
  }
}
