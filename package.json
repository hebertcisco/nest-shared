{
  "name": "nest-shared",
  "version": "5.0.8",
  "description": "Shared code for nestjs projects",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "npx prettier --write \"src/**/*.(js|ts)\"",
    "lint": "npx eslint src --ext .js,.ts",
    "lint:fix": "npx eslint src --fix --ext .js,.ts",
    "test": "npx jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config jest-e2e.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release": "npx semantic-release",
    "release:dry": "npx semantic-release --dry-run"
  },
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/nest-shared.git"
  },
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/nest-shared/issues"
  },
  "homepage": "https://github.com/hebertcisco/nest-shared#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@localtools/base64": "^0.0.4",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.13",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.13",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/swagger": "^7.4.2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-handle": "^0.0.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "qs": "^6.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "unique-slug": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@nestjs/cli": "^10.4.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.0",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.14",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.17.9",
    "@types/qs": "^6.9.17",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "28.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "3.4.2",
    "semantic-release": "^24.2.0",
    "ts-jest": "29.2.5",
    "typescript": "5.7.2"
  }
}
